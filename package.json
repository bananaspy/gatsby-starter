{
  "name": "gatsby-starter",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "dev": "gatsby develop",
    "start": "yarn run dev",
    "serve": "gatsby serve",
    "analyze": "ANALYZE_BUNDLE_SIZE=true gatsby build"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@emotion/core": "^10.0.14",
    "@emotion/styled": "^10.0.14",
    "@mdx-js/mdx": "^1.1.4",
    "@mdx-js/react": "^1.1.4",
    "gatsby": "^2.13.32",
    "gatsby-plugin-emotion": "^4.1.2",
    "gatsby-plugin-mdx": "^1.0.22",
    "gatsby-plugin-react-helmet": "^3.1.2",
    "gatsby-source-filesystem": "^2.1.8",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1"
  },
  "devDependencies": {
    "eslint": "5.3.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.2",
    "husky": ">=1",
    "lint-staged": "^9.2.0",
    "prettier": "^1.18.2"
  }
}
